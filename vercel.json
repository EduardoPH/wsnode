{
  "version": 2,
  "builds": [
    { "src": "src/server/server.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/.*", "dest": "src/server/server.js" },
      {
        "src": "src/keyboardListener",
        "dest": "src/keyboardListener.js"
      }
  ],
  "env": {
    "PORT": "3030"
  }
}
