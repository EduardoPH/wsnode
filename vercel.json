{
  "version": 2,
  "builds": [
    { "src": "src/server/server.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/keyboardListener.js", "dest": "src/keyboardListener.js" },
    { "src": "/renderScreen.js", "dest": "src/renderScreen.js" },
    { "src": "/.*", "dest": "src/server/server.js" }
  ],
  "env": {
    "PORT": "3030"
  }
}
